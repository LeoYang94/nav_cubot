<launch>
  <!--odom & laser>
  <arg name="odom_laser_launch_file" default="$(find nav_cubot)/launch/nav_odom_laser.launch"/>
  <include file="$(arg odom_laser_launch_file)"/>
 
  <!-- Gmapping -->
  <arg name="sensor_gmapping_launch_file" default="$(find nav_cubot)/launch/rplidarA2_gmapping.launch"/>
  <include file="$(arg sensor_gmapping_launch_file)"/>

  <!-- Move base -->
  <include file="$(find nav_cubot)/launch/nav_move_base.launch.xml"/>

</launch>
